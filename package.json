{
	"name": "vscode-star-rod",
	"displayName": "Star Rod",
	"publisher": "nanaian",
	"description": "Language support for the Star Rod Paper Mario modding tool.",
	"version": "0.3.0",
	"license": "MIT",
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#ffef29",
		"theme": "light"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nanaian/vscode-star-rod.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension",
	"activationEvents": [
		"onLanguage:starrod"
	],
	"contributes": {
		"languages": [
			{
				"id": "starrod",
				"aliases": [
					"Star Rod",
					"starrod"
				],
				"extensions": [
					".mscr",
					".mpat",
					".bscr",
					".bpat",
					".wscr",
					".wpat",
					".str",
					".enum",
					".patch"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "starrod",
				"scopeName": "source.starrod",
				"path": "./syntaxes/starrod.tmLanguage.json"
			}
		],
		"configuration": {
			"title": "Star Rod",
			"properties": {
				"starRod.installDirectory": {
					"type": "string",
					"scope": "machine-overridable",
					"markdownDescription": "Absolute path to a [Star Rod 0.2.0 installation](https://github.com/nanaian/star-rod/archive/v0.2.0.zip)."
				}
			}
		}
	},
	"__metadata": {
		"id": "04e91f62-6dcd-4895-ae21-2024b20f3105",
		"publisherDisplayName": "nanaian",
		"publisherId": "8de2bba1-d0d5-431c-acd7-3a20852a6dec"
	},
	"devDependencies": {
		"@types/node": "^13.9.2",
		"@types/vscode": "^1.43.0",
		"typescript": "^3.8.3"
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"makelib": "node lib/make.js > src/lib.json"
	}
}
